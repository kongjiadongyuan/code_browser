<mbr r='argp::args_doc' o='128' t='const char *'/>
<mbr r='argp::doc' o='192' t='const char *'/>
<mbr r='argp::children' o='256' t='const struct argp_child *'/>
<mbr r='argp::help_filter' o='320' t='char *(*)(int, const char *, void *)'/>
<mbr r='argp::argp_domain' o='384' t='const char *'/>
