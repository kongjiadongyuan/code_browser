<mbr r='argp_option::arg' o='128' t='const char *'/>
<mbr r='argp_option::flags' o='192' t='int'/>
<mbr r='argp_option::doc' o='256' t='const char *'/>
<mbr r='argp_option::group' o='320' t='int'/>
