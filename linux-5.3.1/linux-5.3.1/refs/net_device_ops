<mbr r='net_device_ops::ndo_bridge_setlink' o='3136' t='int (*)(struct net_device *, struct nlmsghdr *, u16, struct netlink_ext_ack *)'/>
<mbr r='net_device_ops::ndo_bridge_getlink' o='3200' t='int (*)(struct sk_buff *, u32, u32, struct net_device *, u32, int)'/>
<mbr r='net_device_ops::ndo_bridge_dellink' o='3264' t='int (*)(struct net_device *, struct nlmsghdr *, u16)'/>
<mbr r='net_device_ops::ndo_change_carrier' o='3328' t='int (*)(struct net_device *, bool)'/>
<mbr r='net_device_ops::ndo_get_phys_port_id' o='3392' t='int (*)(struct net_device *, struct netdev_phys_item_id *)'/>
<mbr r='net_device_ops::ndo_get_port_parent_id' o='3456' t='int (*)(struct net_device *, struct netdev_phys_item_id *)'/>
<mbr r='net_device_ops::ndo_get_phys_port_name' o='3520' t='int (*)(struct net_device *, char *, size_t)'/>
<mbr r='net_device_ops::ndo_udp_tunnel_add' o='3584' t='void (*)(struct net_device *, struct udp_tunnel_info *)'/>
<mbr r='net_device_ops::ndo_udp_tunnel_del' o='3648' t='void (*)(struct net_device *, struct udp_tunnel_info *)'/>
<mbr r='net_device_ops::ndo_dfwd_add_station' o='3712' t='void *(*)(struct net_device *, struct net_device *)'/>
<mbr r='net_device_ops::ndo_dfwd_del_station' o='3776' t='void (*)(struct net_device *, void *)'/>
<mbr r='net_device_ops::ndo_get_lock_subclass' o='3840' t='int (*)(struct net_device *)'/>
<mbr r='net_device_ops::ndo_set_tx_maxrate' o='3904' t='int (*)(struct net_device *, int, u32)'/>
<mbr r='net_device_ops::ndo_get_iflink' o='3968' t='int (*)(const struct net_device *)'/>
