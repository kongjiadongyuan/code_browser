<use f='linux-5.3.1/arch/x86/kernel/fpu/regset.c' l='108' u='c' c='xstateregs_get'/>
<use f='linux-5.3.1/arch/x86/kernel/fpu/regset.c' l='308' u='c' c='fpregs_get'/>
<use f='linux-5.3.1/arch/x86/kernel/fpu/regset.c' l='321' u='c' c='fpregs_get'/>
<use f='linux-5.3.1/arch/x86/kernel/ptrace.c' l='711' u='c' c='ioperm_get'/>
