<mbr r='edac_raw_error_desc::top_layer' o='4512' t='int'/>
<mbr r='edac_raw_error_desc::mid_layer' o='4544' t='int'/>
<mbr r='edac_raw_error_desc::low_layer' o='4576' t='int'/>
