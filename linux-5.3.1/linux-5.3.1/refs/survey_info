<mbr r='survey_info::time_busy' o='128' t='u64'/>
<mbr r='survey_info::time_ext_busy' o='192' t='u64'/>
<mbr r='survey_info::time_rx' o='256' t='u64'/>
<mbr r='survey_info::time_tx' o='320' t='u64'/>
<mbr r='survey_info::time_scan' o='384' t='u64'/>
<mbr r='survey_info::filled' o='448' t='u32'/>
