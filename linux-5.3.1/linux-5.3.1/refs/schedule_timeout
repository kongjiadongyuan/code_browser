<use f='linux-5.3.1/kernel/time/timer.c' l='1818' c='schedule_timeout'/>
<use f='linux-5.3.1/kernel/time/timer.c' l='1818' u='a'/>
<use f='linux-5.3.1/kernel/time/timer.c' l='1827' u='c' c='schedule_timeout_interruptible'/>
<use f='linux-5.3.1/kernel/time/timer.c' l='1834' u='c' c='schedule_timeout_killable'/>
<use f='linux-5.3.1/kernel/time/timer.c' l='1841' u='c' c='schedule_timeout_uninterruptible'/>
