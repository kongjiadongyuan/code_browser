<mbr r='i915_hdcp_component_ops::verify_hprime' o='192' t='int (*)(struct device *, struct hdcp_port_data *, struct hdcp2_ake_send_hprime *)'/>
<mbr r='i915_hdcp_component_ops::store_pairing_info' o='256' t='int (*)(struct device *, struct hdcp_port_data *, struct hdcp2_ake_send_pairing_info *)'/>
<mbr r='i915_hdcp_component_ops::initiate_locality_check' o='320' t='int (*)(struct device *, struct hdcp_port_data *, struct hdcp2_lc_init *)'/>
<mbr r='i915_hdcp_component_ops::verify_lprime' o='384' t='int (*)(struct device *, struct hdcp_port_data *, struct hdcp2_lc_send_lprime *)'/>
<mbr r='i915_hdcp_component_ops::get_session_key' o='448' t='int (*)(struct device *, struct hdcp_port_data *, struct hdcp2_ske_send_eks *)'/>
<mbr r='i915_hdcp_component_ops::repeater_check_flow_prepare_ack' o='512' t='int (*)(struct device *, struct hdcp_port_data *, struct hdcp2_rep_send_receiverid_list *, struct hdcp2_rep_send_ack *)'/>
<mbr r='i915_hdcp_component_ops::verify_mprime' o='576' t='int (*)(struct device *, struct hdcp_port_data *, struct hdcp2_rep_stream_ready *)'/>
