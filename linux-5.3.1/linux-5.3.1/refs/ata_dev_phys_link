<def f='linux-5.3.1/drivers/ata/libata-core.c' l='295' ll='304' type='struct ata_link * ata_dev_phys_link(struct ata_device * dev)'/>
<use f='linux-5.3.1/drivers/ata/libata-core.c' l='2132' u='c' c='ata_do_link_spd_horkage'/>
