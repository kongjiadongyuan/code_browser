<mbr r='cfg80211_csa_settings::counter_offsets_presp' o='1344' t='const u16 *'/>
<mbr r='cfg80211_csa_settings::n_counter_offsets_beacon' o='1408' t='unsigned int'/>
<mbr r='cfg80211_csa_settings::n_counter_offsets_presp' o='1440' t='unsigned int'/>
<mbr r='cfg80211_csa_settings::beacon_after' o='1472' t='struct cfg80211_beacon_data'/>
<mbr r='cfg80211_csa_settings::radar_required' o='2560' t='bool'/>
