<mbr r='cfg80211_external_auth_params::ssid' o='80' t='struct cfg80211_ssid'/>
<mbr r='cfg80211_external_auth_params::key_mgmt_suite' o='352' t='unsigned int'/>
<mbr r='cfg80211_external_auth_params::status' o='384' t='u16'/>
<mbr r='cfg80211_external_auth_params::pmkid' o='448' t='const u8 *'/>
