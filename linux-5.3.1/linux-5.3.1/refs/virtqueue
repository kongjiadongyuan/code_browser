<mbr r='virtqueue::vdev' o='256' t='struct virtio_device *'/>
<mbr r='virtqueue::index' o='320' t='unsigned int'/>
<mbr r='virtqueue::num_free' o='352' t='unsigned int'/>
<mbr r='virtqueue::priv' o='384' t='void *'/>
