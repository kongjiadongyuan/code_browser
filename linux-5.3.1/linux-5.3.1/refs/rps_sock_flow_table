<def f='linux-5.3.1/include/linux/netdevice.h' l='700' ll='704'/>
<dec f='linux-5.3.1/include/linux/netdevice.h' l='710' type='struct rps_sock_flow_table *'/>
<use f='linux-5.3.1/include/linux/netdevice.h' l='710'/>
<use f='linux-5.3.1/include/linux/netdevice.h' l='712' c='rps_record_sock_flow'/>
<size>64</size>
<doc f='linux-5.3.1/include/linux/netdevice.h' l='690'>/*
 * The rps_sock_flow_table contains mappings of flows to the last CPU
 * on which they were processed by the application (set in recvmsg).
 * Each entry is a 32bit value. Upper part is the high-order bits
 * of flow hash, lower part is CPU number.
 * rps_cpu_mask is used to partition the space, depending on number of
 * possible CPUs : rps_cpu_mask = roundup_pow_of_two(nr_cpu_ids) - 1
 * For example, if 64 CPUs are possible, rps_cpu_mask = 0x3f,
 * meaning we use 32-6=26 bits for the hash.
 */</doc>
<mbr r='rps_sock_flow_table::mask' o='0' t='u32'/>
<mbr r='rps_sock_flow_table::ents' o='512' t='u32 [0]'/>
