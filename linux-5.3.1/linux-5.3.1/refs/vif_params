<mbr r='vif_params::macaddr' o='64' t='u8 [6]'/>
<mbr r='vif_params::vht_mumimo_groups' o='128' t='const u8 *'/>
<mbr r='vif_params::vht_mumimo_follow_addr' o='192' t='const u8 *'/>
