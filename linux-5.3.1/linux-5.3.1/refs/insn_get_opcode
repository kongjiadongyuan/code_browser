<dec f='linux-5.3.1/arch/x86/include/asm/insn.h' l='90' type='void insn_get_opcode(struct insn * insn)'/>
<use f='linux-5.3.1/arch/x86/events/intel/lbr.c' l='901' u='c' c='branch_type'/>
<use f='linux-5.3.1/arch/x86/lib/insn-eval.c' l='39' u='c' c='is_string_insn'/>
<def f='linux-5.3.1/arch/x86/lib/insn.c' l='200' ll='243' type='void insn_get_opcode(struct insn * insn)'/>
<use f='linux-5.3.1/arch/x86/lib/insn.c' l='260' u='c' c='insn_get_modrm'/>
<doc f='linux-5.3.1/arch/x86/lib/insn.c' l='190'>/**
 * insn_get_opcode - collect opcode(s)
 * @insn:	&amp;struct insn containing instruction
 *
 * Populates @insn-&gt;opcode, updates @insn-&gt;next_byte to point past the
 * opcode byte(s), and set @insn-&gt;attr (except for groups).
 * If necessary, first collects any preceding (prefix) bytes.
 * Sets @insn-&gt;opcode.value = opcode1.  No effect if @insn-&gt;opcode.got
 * is already 1.
 */</doc>
<dec f='linux-5.3.1/tools/objtool/arch/x86/include/asm/insn.h' l='90' type='void insn_get_opcode(struct insn * insn)'/>
