<def f='linux-5.3.1/include/linux/dma-fence.h' l='395' ll='407' type='bool dma_fence_is_signaled(struct dma_fence * fence)'/>
<use f='linux-5.3.1/include/linux/dma-fence.h' l='469' u='c' c='dma_fence_later'/>
