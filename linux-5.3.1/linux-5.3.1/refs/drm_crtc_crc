<mbr r='drm_crtc_crc::entries' o='192' t='struct drm_crtc_crc_entry *'/>
<mbr r='drm_crtc_crc::head' o='256' t='int'/>
<mbr r='drm_crtc_crc::tail' o='288' t='int'/>
<mbr r='drm_crtc_crc::values_cnt' o='320' t='size_t'/>
