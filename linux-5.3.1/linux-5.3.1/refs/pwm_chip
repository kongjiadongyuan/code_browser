<mbr r='pwm_chip::npwm' o='160' t='unsigned int'/>
<mbr r='pwm_chip::of_xlate' o='192' t='struct pwm_device *(*)(struct pwm_chip *, const struct of_phandle_args *)'/>
<mbr r='pwm_chip::of_pwm_n_cells' o='256' t='unsigned int'/>
<mbr r='pwm_chip::list' o='320' t='struct list_head'/>
