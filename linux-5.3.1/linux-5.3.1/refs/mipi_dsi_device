<mbr r='mipi_dsi_device::lanes' o='5952' t='unsigned int'/>
<mbr r='mipi_dsi_device::format' o='5984' t='enum mipi_dsi_pixel_format'/>
<mbr r='mipi_dsi_device::mode_flags' o='6016' t='unsigned long'/>
<mbr r='mipi_dsi_device::hs_rate' o='6080' t='unsigned long'/>
