<doc f='linux-5.3.1/include/linux/ieee80211.h' l='24'>/*
 * DS bit usage
 *
 * TA = transmitter address
 * RA = receiver address
 * DA = destination address
 * SA = source address
 *
 * ToDS    FromDS  A1(RA)  A2(TA)  A3      A4      Use
 * -----------------------------------------------------------------
 *  0       0       DA      SA      BSSID   -       IBSS/DLS
 *  0       1       DA      BSSID   SA      -       AP -&gt; STA
 *  1       0       BSSID   SA      DA      -       AP &lt;- STA
 *  1       1       RA      TA      DA      SA      unspecified (WDS)
 */</doc>
